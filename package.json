{
  "name": "@phanect/eslint-plugin",
  "version": "2019.3.25",
  "description": "ESLint configuration for Jumpei Ogawa (@phanect)'s private projects",
  "main": "config.js",
  "scripts": {
    "test": "eslint . && ./tests/run.sh",
    "clean": "git clean -dX --force",
    "postinstall": "rm -f ./node_modules/@phanect/eslint-plugin && mkdir -p ./node_modules/@phanect && ln --symbolic ../../ ./node_modules/@phanect/eslint-plugin"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:phanect/eslint-config-phanective.git"
  },
  "author": "Jumpei Ogawa",
  "license": "CC0-1.0",
  "engines": {
    "node": ">=6.x"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest",
    "eslint-config-standard": "latest",
    "eslint-plugin-security": "latest",
    "eslint-plugin-vue": "latest",
    "typescript": "latest"
  }
}
